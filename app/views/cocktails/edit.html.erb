<h1>edit <%= @cocktail.name %></h1>

<div class="container">
  <h1 class="text-center">Add a cocktail</h1>
  <div class="row">
   <div class="col-xm-12 col-md-12 col-lg-12">
<%= simple_form_for @cocktail do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit %>
<% end %>
    </div>
  </div>
</div>
